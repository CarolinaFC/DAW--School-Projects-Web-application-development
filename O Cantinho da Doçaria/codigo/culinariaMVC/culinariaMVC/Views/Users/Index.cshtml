@model IEnumerable<culinariaMVC.Models.User>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/Layout_AdminHome.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tipo_user)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tipo_user)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_user">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_user">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_user">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
